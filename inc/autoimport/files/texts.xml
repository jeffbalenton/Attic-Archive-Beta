							<wp:postmeta>
		<wp:meta_key><![CDATA[_text_transcriptions_2_text_transcription_section]]></wp:meta_key>
		<wp:meta_value><![CDATA[field_5f514b76ec38e]]></wp:meta_value>
		</wp:postmeta>
							<wp:postmeta>
		<wp:meta_key><![CDATA[text_transcriptions_2_text_transcription_page_image]]></wp:meta_key>
		<wp:meta_value><![CDATA[3577]]></wp:meta_value>
